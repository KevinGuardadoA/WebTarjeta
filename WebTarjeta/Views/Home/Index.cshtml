@model List<Movimiento>
@{
    ViewData["Title"] = "Home Page";
}

<div class="card">
    <div class="card-header">
        Buscar Tarjeta
    </div>
    <div class="card-body">

        
        
        <form asp-controller="Home" asp-action="NumeroTarjeta">
            <div class="mb-3">
                <label for="NumeroTarjeta" class="form-label">NumeroTarjeta</label>
                <input type="number" class="form-control" id="NumeroTarjeta" name="NumeroTarjeta" />
            </div>
            <button type="submit" class="btn btn-primary">Buscar</button>
        </form>
        
    </div>
</div>
